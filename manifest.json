{
  "manifest_version": 3,
  "name": "Form Autofill Extension",
  "version": "1.0",
  "description": "Automates form filling and stores personal information.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Form Autofill Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
